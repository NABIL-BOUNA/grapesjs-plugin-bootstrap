{
  "name": "grapesjs-plugin-bootstrap",
  "version": "0.1.330",
  "description": "GrapesJS Plugin Bootstrap",
  "main": "dist/grapesjs-plugin-bootstrap.min.js",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "build": "npm run v:patch && npm run standard && webpack --env.production",
    "start": "webpack-dev-server --open --progress --colors",
    "standard": "standard --fix \"./src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "boilerplate",
    "wysiwyg"
  ],
  "author": "olivmonnier",
  "license": "BSD-3-Clause",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "peerDependencies": {
    "grapesjs": "0.x"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.1.1",
    "html-webpack-plugin": "^2.30.1",
    "standard": "^10.0.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
